{
    "name": "cupoftea/laravel-git-wrapper",
    "description": "Laravel GitWrapper is a GitWrapper bridge for Laravel 5",
    "keywords": ["laravel", "framework", "git", "git wrapper", "git wrapper bridge", "bridge", "Laravel GitWrapper", "Laravel-GitWrapper", "CupOfTea", "Frankie Wittevrongel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Frankie Wittevrongel",
            "email": "frankie@cupoftea.io"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "cpliakas/git-wrapper": "^2.1",
        "cupoftea/package": "^1.4",
        "graham-campbell/manager": "^4.1",
        "illuminate/support": "^5.5 <5.9"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.1",
        "phpunit/phpunit": "^6.5|^7.0"
    },
    "autoload": {
        "psr-4": {
            "CupOfTea\\GitWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CupOfTea\\Tests\\GitWrapper\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CupOfTea\\GitWrapper\\GitWrapperServiceProvider"
            ]
        }
    }
}
